// @font-face declarations

@font-face {
		font-family: 'Suisse';
		src: url('/fonts/SuisseIntl/SuisseIntl-RegularItalic.woff2') format('woff2'),
				url('/fonts/SuisseIntl/SuisseIntl-RegularItalic.woff') format('woff');
		font-weight: normal;
		font-style: italic;
}

@font-face {
		font-family: 'Suisse';
		src: url('/fonts/SuisseIntl/SuisseIntl-Regular.woff2') format('woff2'),
				url('/fonts/SuisseIntl/SuisseIntl-Regular.woff') format('woff');
		font-weight: normal;
		font-style: normal;
}

@font-face {
		font-family: 'Suisse';
		src: url('/fonts/SuisseIntl/SuisseIntl-Medium.woff2') format('woff2'),
				url('/fonts/SuisseIntl/SuisseIntl-Medium.woff') format('woff');
		font-weight: 500;
		font-style: normal;
}
